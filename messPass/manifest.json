{
  "name": "Pass messages",
  "description": "Pass message from content_script to background",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],

  "background": {
    "page": "background.html"
//    "scripts": ["background.js"]
    
  },

  "browser_action": {
    "default_title": "Pass message",
    "default_popup": "popup.html"
},
"content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content_script.js", "background.js"]
    }
  ],
  "manifest_version": 2
}
